# Generate your own secrets! (e.g. node -e "console.log(crypto.randomBytes(32).toString('hex'))")
JWT_ACCESS_SECRET=
JWT_REFRESH_SECRET=

# LiveKit (runs natively, not in Docker)
# Dev: ws://localhost:7890 (start-livekit.mjs runs it locally)
# Prod: ws://YOUR_PUBLIC_IP:7880 (generated by pnpm setup)
LIVEKIT_URL=ws://localhost:7890
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=dev-secret-do-not-use-in-production!

# Postgres credentials
POSTGRES_USER=migo
POSTGRES_PASSWORD=
POSTGRES_DB=migo
